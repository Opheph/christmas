ALIYUN_AI_API_URL=https://your.aliyun.ai/endpoint
ALIYUN_AI_API_KEY=your_api_key_here
ALIYUN_AI_API_KEY_HEADER=Authorization

# Dashscope (OpenAI-compatible) example
# Note: by default the app runs in local echo mode and does not call Dashscope.
# To enable real Dashscope calls, set DASHSCOPE_API_KEY and optionally DASHSCOPE_BASE_URL / DASHSCOPE_MODEL
DASHSCOPE_API_KEY=your_dashscope_api_key_here
# Optional: override if your Dashscope endpoint is different (region specific)
DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
DASHSCOPE_MODEL=qwen-plus

# Copy this file to .env.local and set the real values.
# Never commit your real .env.local to version control.

# Optional background music (public URL). If not set, a default public track will be used.
# Use `NEXT_PUBLIC_MUSIC_URL` to make it available to the client.
NEXT_PUBLIC_MUSIC_URL=https://upload.wikimedia.org/wikipedia/commons/8/8d/We_Wish_You_a_Merry_Christmas.ogg
